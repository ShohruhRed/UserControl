@model IEnumerable<UserControl.Data.ApplicationUser>

<h2>Список пользователей</h2>
<table class="table">
    @foreach (var user in Model)
    {
        <tr>
            <td>@user.Email</td>
            <td>
                <a class="btn btn-sm btn-primary" asp-action="Edit" asp-route-userid="@user.Id">Permission</a>
            </td>
        </tr>
    }
</table>